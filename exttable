CREATE EXTERNAL TABLE IF NOT EXISTS marc.students(
  student_ID INT, FirstName STRING, LastName STRING,    
  year STRING, Major STRING)
  COMMENT 'Student Names'
  ROW FORMAT DELIMITED
  FIELDS TERMINATED BY ','
  STORED AS TEXTFILE
  LOCATION 'hdfs://base3.clouderalab.local/warehouse/tablespace/external/hive/marc';
